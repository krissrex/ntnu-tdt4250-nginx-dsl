grammar no.ntnu.tdt4250.Nginx with org.eclipse.xtext.common.Terminals

generate nginx "http://www.ntnu.no/tdt4250/Nginx"

Nginx: 
	(sites+=Site)*;

terminal SITE_NAME: 
	('default' | ID ('.' ID)*);

Site: 
	name=SITE_NAME ':' enries+=Entry*;
	
terminal VALUE: (INT | ID);

Entry:
	name=ID ':' value=VALUE;

